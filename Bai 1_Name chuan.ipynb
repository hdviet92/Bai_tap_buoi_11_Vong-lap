    "fullname = input('Nhập họ và tên: ').split()\n",
    "fix = ''\n",
    "for i in fullname:\n",
    "    if i.isalpha():\n",
    "        fix += i[0].upper() + i[1:].lower() + ' '\n",
    "fix = fix.strip()\n",
    "print('Họ và tên: ',fix)\n"
   ]
